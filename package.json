{
  "name": "hackney-cinema-calendar",
  "version": "1.0.0",
  "private": true,
  "description": "Automatically generated calendar of events for cinemas in Hackney, London",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "format": "prettier \"./**/*.js(on)?\" --write && prettier \"./**/*.md\" --write --prose-wrap=always",
    "generate:picturehouses.com": "TZ=Europe/London node index.js picturehouses.com",
    "generate:riocinema.org.uk": "TZ=Europe/London node index.js riocinema.org.uk",
    "generate:thecastlecinema.com": "TZ=Europe/London node index.js thecastlecinema.com",
    "generate:all": "npm run generate:picturehouses.com && npm run generate:riocinema.org.uk && npm run generate:thecastlecinema.com"
  },
  "author": "Alistair Brown <github@alistairjcbrown.com>",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.6.0",
    "ics": "^3.7.6",
    "replace-special-characters": "^1.2.7",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  }
}
